<?xml version="1.0"?>
<package >
  <metadata>
    <id>Prius.MySql</id>
    <version>2.1.1</version>
    <title>Prius driver for MySQL and MariaDB databases</title>
    <authors>Martin Halliday</authors>
    <owners>Martin Halliday</owners>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>Configure databases in Prius ORM to connect to MySql and MariaDB. Defines the server type 'MySql' that you can use in your Prius ORM configuration file.</description>
    <copyright>Copyright 2009-2019</copyright>
    <releaseNotes>First release, not fully tested yet</releaseNotes>
    <projectUrl>https://github.com/Bikeman868/Prius</projectUrl>
    <license type="expression">Apache-2.0</license>
    <tags>ORM database prius mysql</tags>
    <dependencies>
      <group targetFramework="net40">
        <dependency id="Ioc.Modules.Contract" version="1.4.1" />
        <dependency id="Prius" version="2.1.1" />
        <dependency id="MySql.Data" version="6.9.9" />
      </group>
      <group targetFramework="net45">
        <dependency id="Ioc.Modules.Contract" version="1.4.1" />
        <dependency id="Prius" version="2.1.1" />
        <dependency id="MySql.Data" version="6.9.9" />
      </group>
    </dependencies>
    <references>
      <reference file="Prius.MySql.dll"/>
    </references>
  </metadata>
  <files>
    <file src="Prius.MySql\bin\Net40\Release\Prius.MySql.dll" target="lib\net40"/>
    <file src="Prius.MySql\bin\Net40\Release\Prius.MySql.dll" target="ref\net40"/>
    <file src="Prius.MySql\bin\Net40\Release\Prius.MySql.pdb" target="lib\net40"/>

    <file src="Prius.MySql\bin\Net45\Release\Prius.MySql.dll" target="lib\net45"/>
    <file src="Prius.MySql\bin\Net45\Release\Prius.MySql.dll" target="ref\net45"/>
    <file src="Prius.MySql\bin\Net45\Release\Prius.MySql.pdb" target="lib\net45"/>
    
    <file src="*.md" target=""/>
    <file src="*.txt" target=""/>
  </files>
</package>