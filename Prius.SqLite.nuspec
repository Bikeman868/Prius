<?xml version="1.0"?>
<package >
  <metadata>
    <id>Prius.SQLite</id>
    <version>2.2.0</version>
    <title>Prius driver for SQLite databases</title>
    <authors>Martin Halliday</authors>
    <owners>Martin Halliday</owners>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>Configure databases in Prius ORM to connect to SQLite databases. Defines the server type 'SQLite' that you can use in your Prius ORM configuration file.</description>
    <copyright>Copyright 2009-2019</copyright>
    <releaseNotes>Fixes for bugs discovered in initial deployment</releaseNotes>
    <projectUrl>https://github.com/Bikeman868/Prius</projectUrl>
    <license type="expression">Apache-2.0</license>
    <tags>ORM database prius sqlite</tags>
    <dependencies>
      <group targetFramework="net40">
        <dependency id="Ioc.Modules.Contract" version="1.4.1" />
        <dependency id="Prius" version="2.2.0" />
        <dependency id="System.Data.SQLite.Core" version="1.0.108" />
      </group>
      <group targetFramework="net45">
        <dependency id="Ioc.Modules.Contract" version="1.4.1" />
        <dependency id="Prius" version="2.2.0" />
        <dependency id="System.Data.SQLite.Core" version="1.0.108" />
      </group>
    </dependencies>
    <references>
      <reference file="Prius.SQLite.dll"/>
    </references>
  </metadata>
  <files>
    <file src="Prius.SQLite\bin\Net40\Release\Prius.SQLite.dll" target="lib\net40"/>
    <file src="Prius.SQLite\bin\Net40\Release\Prius.SQLite.pdb" target="lib\net40"/>
    
    <file src="Prius.SQLite\bin\Net45\Release\Prius.SQLite.dll" target="lib\net45"/>
    <file src="Prius.SQLite\bin\Net45\Release\Prius.SQLite.pdb" target="lib\net45"/>

    <file src="Prius.SQLite\lib\x86\SQLite.Interop.dll" target="build\x86"/>
    <file src="Prius.SQLite\lib\x64\SQLite.Interop.dll" target="build\x64"/>

    <file src="Prius.SQLite\bin\Net40\Release\Prius.SQLite.dll" target="ref\net40"/>
    <file src="Prius.SQLite\bin\Net45\Release\Prius.SQLite.dll" target="ref\net45"/>

    <file src="*.md" target=""/>
    <file src="*.txt" target=""/>
  </files>
</package>